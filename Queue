#include<iostream>
using namespace std;

class Queue {
	public:
		int front, back;
		int que[5];
		Queue(){
			front=-1;
			back=-1;
		};
		void enQueue(int data);
		void deQueue();
		void display();
};

void Queue::enQueue(int data){
	if(front==-1)
		front=0;
	back=back+1;
	que[back]=data;
	display();
}

void Queue::deQueue(){
	if (front ==-1 || front>back){
		cout<<" Queue empty";
		front=-1;
		back=-1;
	}
	else
		front++;
	display();	
}


void Queue::display(){
	for(int i=front;i<=back;i++)
		cout<<" "<<que[i]<<" ";
	cout<<endl;
}

int main(){
	Queue q;
	q.enQueue(1);
	q.enQueue(2);
	q.deQueue();
	q.enQueue(3);
	q.enQueue(4);
	q.enQueue(5);
	q.enQueue(6);
	q.deQueue();
	return 0;
}
