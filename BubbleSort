#include<iostream>
using namespace std;

template<typename T>
void bubble (T *const array, int const num) {
	for (int i = num;i>=0;i--){
		for(int j=0;j<i;j++){
			if(array[j]>array[j+1]){
				swap(array[j],array[j+1]);
			}
		}
	}
}
int main(){
	int array[10] = {11,23,51,24,64,41,35,65,46,32};
	char Carray[11] = {'f','u','r','k','a','n','a','y','g','u','r'};
	int number;
	for(int i=0;i<10;i++){
		cout<<array[i]<<" ";
	}
	cout<<endl;
	cout<<"Number : ";
	cin>>number;
	bubble(array,number);
	for(int i=0;i<number;i++){
		cout<<array[i]<<" ";
	}
	cout<<endl;
	for(int i=0;i<11;i++){
		cout<<Carray[i]<<" ";
	}
	cout<<endl;
	cout<<"Number : ";
	cin>>number;
	bubble(Carray,number);
	for(int i=0;i<number;i++){
		cout<<Carray[i]<<" ";
	}
	
	return 0;
}
